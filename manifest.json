{
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html",
      "default_title": "shiv"
   },
   "content_scripts": [ {
      "all_frames": true,
      "css": [ "style.css" ],
      "js": ["jquery.js","main.js"],
      "matches": [ "*://www.geeksforgeeks.org/*","*://geeksquiz.com/*"]
   } ],
   "description": "Mark Your Notes by highlighting text",
   "icons": {
      "48": "icon_48x48.png"
   },
   "manifest_version": 2,
   "name": "GeekMarko",
   "permissions": [ "activeTab", "tabs", "storage", "identity","unlimitedStorage" ],
   "short_name": "shiv",
   "version": "1.2.0.4",
   "web_accessible_resources": [ "funcs.js", "jquery.js", "jquery-ui.js" , "listeners.js"]
}
